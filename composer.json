{
    "name": "portrino/codeception-yandex-module",
    "description": "Provides validation of structured data via yandex API",
    "authors": [
        {
            "name": "Andr√© Wuttig",
            "email": "wuttig@portrino.de"
        }
    ],
    "require": {
        "codeception/codeception": "~2.3",
        "flow/jsonpath": "~0.3",
        "nixsolutions/yandex-php-library": "~3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.4",
        "squizlabs/php_codesniffer": "~3.0.2",
        "phpstan/phpstan": "^0.8"
    },
    "autoload": {
        "psr-4": {
            "Codeception\\Module\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codeception\\Module\\Tests\\": "tests"
        }
    },
    "config": {
        "vendor-dir": "build/vendor",
        "bin-dir": "build/bin"
    }
}
